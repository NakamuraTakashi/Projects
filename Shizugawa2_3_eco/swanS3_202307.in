PROJECT 'Shizugawa3' '01'
 'Shizugawa3 ROMS-SWAN coupling'
 'Bathymetry: MyBath'
 'COMMENTS'

MODE NONSTATIONARY TWODIMENSIONAL

SET DEPMIN 0.10 INRHOG 1 CARTESIAN
COORDINATES CARTESIAN

&& KEYWORD for number of nested SWAN grids.
NSGRIDS 1

&& KEYWORDS TO CREATE AND READ COMPUTATIONAL GRID &&
CGRID REGULAR 537468.92576650 4277698.809640 -18.907607 8760.000000 10260.000000 146 171 &
        CIRCLE 36 0.04 1.0 20

&& KEYWORDS TO CREATE AND READ BATHYMETRY GRID &&
INPGRID BOTTOM REGULAR 537468.925767 4277698.809640 -18.907607 146 171 60.000000 60.000000 EXC 9.999000e+003
READINP BOTTOM  1 '../../COAWST_DATA/Shizugawa/Shizugawa3/Grid/Shizugawa3_swan_grd_v0.5b.bot' 4 0 FREE

&& KEYWORD TO CREATE CURRENT GRID &&
INPGRID CURRENT REGULAR 537468.925767 4277698.809640 -18.907607 146 171 60.000000 60.000000 EXC 9.999000e+003   &
        NONSTAT  19900101.000000 21915 DAY 20500101.000000

&& KEYWORD TO CREATE WATER LEVEL GRID &&
INPGRID WLEV REGULAR 537468.925767 4277698.809640 -18.907607 146 171 60.000000 60.000000 EXC 9.999000e+003      &
        NONSTAT  19900101.000000 21915 DAY 20500101.000000

&& KEYWORD TO CREATE BOTTOM FRICTION GRID &&
INPGRID FRIC REGULAR 537468.925767 4277698.809640 -18.907607 146 171 60.000000 60.000000 EXC 9.999000e+003      &
        NONSTAT  19900101.000000 21915 DAY 20500101.000000

&& KEYWORD TO CREATE WIND GRID &&
INPGRID WIND REGULAR 492400.000000 4215000.000000 -18.907607 101 161 1500.000000 1500.000000 EXC 9.999000e+003  &
        NONSTAT 20230601.000000 1 HR 20231001.230000
READINP WIND 1 SERIES 'wind_series_202306.dat' 4 0 1 1 FREE

&& KEYWORD TO CREATE NPLANTS VEGETATION GRID &&
INPGRID NPLANTS REGULAR 537468.925767 4277698.809640 -18.907607 146 171 60.000000 60.000000 EXC 9.999000e+003

&& VEGEtation  < [height] [diamtr] [nstems] [drag] >
&& Vegetation parameters set here are to be consistent
&& with the ones in ROMS input file 
VEGETATION 2.0 0.3 1 1.0 
&&
&& New for SWAN 41.45 need to have an iveg number
&&  < [iveg = 1 or 2] [height]  [diamtr]  [nstems]  [drag]>
&VEGETATION 1 10.0 0.1 1 1.0 
&&
&&  to use coawst you need
&&  < [height]  [diamtr]  [nstems]  [drag]  [youngsmod]>
&VEGETATION 10.0 0.1 1 1.0 1.0e9

& BOUNDARY FORCING &&
BOUNDNEST1  NEST '../../COAWST_DATA/Shizugawa/Shizugawa3/wave/nest/Shizugawa3_swan_nest_20230605.nst'  CLOSED

& Restart name **********************************
&INIT HOTSTART '../../COAWST_OUTPUT/Shizugawa/Shizugawa3/Shizugawa3_swan_rst_20210725.dat'

& PHYSICS  **************************************
GEN3 KOMEN
FRICTION MADSEN 0.05
BREAKING CONSTANT 1.0
WCAP KOM 2.36E-5 3.02E-3 2.0 1.0 1.0

&BLOCK 'COMPGRID' NOHEADER '../../COAWST_OUTPUT/Shizugawa/Shizugawa3/Shizugawa3_swan_Hsig_20210107.mat' LAY 4 HSIGN 1.0 OUTPUT 20210101.000000 1 HR
&BLOCK 'COMPGRID' NOHEADER '../../COAWST_OUTPUT/Shizugawa/Shizugawa3/Shizugawa3_swan_Tpk__20210107.mat' LAY 4 RTP   1.0 OUTPUT 20210101.000000 1 HR
&BLOCK 'COMPGRID' NOHEADER '../../COAWST_OUTPUT/Shizugawa/Shizugawa3/Shizugawa3_swan_Dpk__20210107.mat' LAY 4 PDIR  1.0 OUTPUT 20210101.000000 1 HR
&BLOCK 'COMPGRID' NOHEADER '../../COAWST_OUTPUT/Shizugawa/Shizugawa3/Shizugawa3_swan_Wind_20210107.mat' LAY 4 WIND  1.0 OUTPUT 20210101.000000 1 HR

PROP BSBT
RESTART '../../COAWST_OUTPUT/Shizugawa/SZ3_eco_oys_test1/SZ3_swan_rst_20230702.dat' FREE 1 DAY

COMPUTE NONSTATIONARY 20230702.000000 600 SEC 20230901.000000

&HOTFILE '../../COAWST_OUTPUT/Shizugawa/Shizugawa3/Shizugawa3_swan_20210107.hot'

STOP
